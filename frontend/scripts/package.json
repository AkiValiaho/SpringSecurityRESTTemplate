{
  "name": "springtemplate",
  "version": "1.0.0",
  "description": "Spring REST Template with auto-building react frontend",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "npm install",
    "build": "browserify -t [ babelify --presets [ react ] ] application.js -o bundle.js && gulp",
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AkiValiaho/SpringSecurityRESTTemplate.git"
  },
  "keywords": [
    "Aki",
    "Valiaho"
  ],
  "author": "Aki Valiaho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AkiValiaho/SpringSecurityRESTTemplate/issues"
  },
  "homepage": "https://github.com/AkiValiaho/SpringSecurityRESTTemplate#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.12",
    "angular2-google-maps": "^0.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bower": "^1.7.7",
    "es6-shim": "^0.35.0",
    "gulp": "^3.9.1",
    "gulp-util": "^3.0.7",
    "gulp-zip": "^3.2.0",
    "jasmine-node": "^1.14.5",
    "ng2-bootstrap": "^1.0.7",
    "reactify": "^1.1.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.3",
    "systemjs": "^0.19.24",
    "zone.js": "^0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9"
  }
}
